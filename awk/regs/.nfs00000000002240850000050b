[7m[abpc10165][27m /acc/src/dsc/drivers/cohtdrep/lewis/coht/cvora/awk/regs $ more q4.awk 
#!/bin/awk -f

BEGIN { print "#!/bin/awk -f";
	print "";
	pname = 1;
      }

{
   if (pname==1) {
      pname = 0;
      print "BEGIN { print \"NAME " $2 "\"}";
   }
}

{ print "{ if ($1 == " $1 ") print $1 \" \" $2 \" \" $3 \" \" $4 \" \" $5 \" \" $6; }" };

END { print "";
      print "END {}";
    }
[7m[abpc10165][27m /acc/src/dsc/drivers/cohtdrep/lewis/coht/cvora/awk/regs $ more q6.awk 
#!/bin/awk -f
BEGIN { }

{
   if ($1 == "NAME") print $1 " " $2;
   else {

      printf("$ %s ",$2);
      printf("\t");

      if ($3 == "w") printf("WO ");
      else if (($3 == "r" ) || ($3 == "rc")) printf("RO ");
      else printf("RW ");
      printf("\t");

      printf("%s ",$4);
      printf("\t");

      if ($5 == "long") printf("4 ");
      else if ($5 == "short") printf("2 ");
      else printf("1 ");
      printf("\t");

      printf("1 ");
      printf("\t");

      printf("%s\n", $6);
   }
}

END { }
