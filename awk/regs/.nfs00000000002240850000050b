[H[2J[H[?1049h[?1h=[?12;25h[H[2J[H=[2l[H[H[2J[1;6H+   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   +   ++----+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+---+--+[60;1H+-------------------------------------------------------------------------------------------------------------------------------------------------------+[60;76H+[62;35HAt[62;42H1x1   inINSERT[A[K[61;96H[KEDIT: genmake.awk[2;1H|#!/bin/awk -f[2;153H|[3;1H|[3;153H|[4;1H|BEGIN {[4;153H|[5;1H|   print "#include <vmeio_support.h>\n"[5;153H|[6;1H|}[6;153H|[7;1H|[7;153H|[8;1H|{[8;153H|[9;1H|   if ($1 == "NAME") {[9;153H|[10;1H|[10;8Hformat = "%Y %a %B-%e %H:%M:%S";[10;153H|[11;1H|[11;8Hprint("");[11;153H|[12;1H|[12;8Hprint("/**");[12;153H|[13;1H|[13;8Hprint(" * Library for " toupper($2) " module ");[13;153H|[14;1H|[14;8Hprint(" * Generated automatically by genlib.c.awk at:" strftime(format));[14;153H|[15;1H|[15;8Hprint(" */");[15;153H|[16;1H|[16;8Hprint("");[16;153H|[17;1H|[17;8Hprint("#include <lib" tolower($2) ".h>");[17;153H|[18;1H|[18;8Hprint("");[18;153H|[19;1H|   }[19;153H|[20;1H|[20;153H|[21;1H|   if ($1 == "$") {[21;153H|[22;1H|[22;8Hprint ""[22;153H|[23;1H|[23;153H|[24;1H|[24;8Hif ($3 == "RW") { loops = 2; set = 1; }[24;153H|[25;1H|[25;8Hif ($3 == "RO") { loops = 1; set = 0; }[25;153H|[26;1H|[26;8Hif ($3 == "WO") { loops = 1; set = 1; }[26;153H|[27;1H|[27;153H|[28;1H|[28;8Hfor (i=0; i<loops; i++) {[28;153H|[29;1H|[29;11Hif (set == 1) {[29;153H|[30;1H|[30;14Hprintf("%s","int SET_");[30;153H|[31;1H|[31;11H} else {[31;153H|[32;1H|[32;14Hprintf("%s","int GET_");[32;153H|[33;1H|[33;11H}[33;153H|[34;1H|[34;11Hif ($7 == "1") {[34;153H|[35;1H|[35;14Hif ($5 == "8") print $2 "(void *handle, long *" tolower($2) ") 