= cvora.regs[K[10;153H|[11;8H[K[11;153H|[11;103H[K[11;153H|[12;2HCPU=L865[K[12;153H|[13;8H[K[13;153H|[13;103H[K[13;153H|[14;2Hinclude /ps/dsc/src/co/Make.auto[K[14;153H|[15;8H[K[15;153H|[15;103H[K[15;153H|[16;2HCFLAGS= -g -Wall -I. -I.. -I../driver -I../driver/support[17;8H[K[17;153H|[17;103H[K[17;153H|[18;2HSRCS=$(LIBNAME[C.c $(LIBNAME).h $(REGFILE)[19;5H[K[19;153H|[19;100H[K[19;153H|[20;2Hall: $(LIBNAME).$(CPU).a $(LIBNAME).$(CPU).so[21;5H[K[21;153H|[21;100H[K[21;153H|[22;2H$(LIBNAME).$(CPU).o: $(SRCS)[23;153H[24;2H$(LIBNAME).h: $(REGFILE) genlib.h.awk[K[24;153H|[25;8H  cat $(REGFILE) | ./genlib.h.awk > $(LIBNAME).h[26;8H[K[26;153H|[26;103H[K[26;153H|[27;2H$(LIBNAME).c: $(REGFILE) genlib.c.awk[28;8H  cat $(REGFILE) | ./genlib.c.awk > $(LIBNAME).c[29;11H[K[29;153H|[29;106H[K[29;153H|[30;2H$(LIBNAME).$(CPU).a: $(LIBNAME).$(CPU).o[31;10H-$(RM) $@[32;10H$(AR) $(ARFLAGS) $@ $^[K[32;153H|[33;10H$(RANLIB) $@[34;11H[K[34;153H|[34;106H[K[34;153H|[35;2H$(LIBNAME).$(CPU).so: $(LIBNAME).[C(CPU).o[K[35;153H|[36;10H-$(RM) $@[K[36;153H|[37;10H$(CC) $[CCFLAGS) -o $@ -shared $^[K[37;153H|[38;14H[K[38;153H|[38;109H[K[38;153H|[39;2Hclean:[K[39;153H|[40;10Hrm -f $(LIBNAME).$(CPU).o[41;10Hrm -f $[CLIBNAME).$(CPU).a[K[41;153H|[42;10Hrm -f $[CLIBNAME).$(CPU).so[K[42;153H|[43;10Hrm -f $[CLIBNAME).h[K[43;153H|[44;10Hrm -f $[CLIBNAME).c[K[44;153H|[45;1H;[45;11H[K[45;153H|[45;106H[K[45;153H|[46;1H;[46;11H[K[46;153H|[46;106H[K[46;153H|[47;1H;[47;11H[K[47;153H|[47;106H[K[47;153H|[48;1H;[48;11H[K[48;153H|[48;106H[K[48;153H|[49;1H;[49;11H[K[49;153H|[49;106H[K[49;153H|[50;1H;[50;11H[K[50;153H|[50;106H[K[50;153H|[51;1H;[51;11H[K[51;153H|[51;106H[K[51;153H|[52;1H;[52;11H[K[52;153H|[52;106H[K[52;153H|[53;1H;[53;11H[K[53;153H|[53;106H[K[53;153H|[54;1H;[54;11H[K[54;153H|[54;106H[K[54;153H|[55;1H;[55;8H[K[55;153H|[55;103H[K[55;153H|[56;1H;   [K[56;153H|[56;100H[K[56;153H|[57;1H;[K[57;153H|[57;97H[K[57;153H|[58;1H;[58;153H
;[K[59;153H|[59;97H[K[59;153H|[61;1H[K[61;96H[K[K[62;51HMakefile   [2;2H[C!/bin/awk -f[K[2;153H|[3;2H[K[3;153H|[3;97H[K[3;153H|[4;2HBEGIN {[K[4;153H|[5;5Hprint "#include <vmeio_support.h>\n"[6;2H}[K[6;153H|[7;2H[K[7;153H|[7;97H[K[7;153H|[8;2H{[9;2H   if ($1 == "NAME") {[10;2H      format = "%Y %a %B-%e %H:%M:%S";[11;8Hprint("");[12;2H      print("/**");[13;8Hprint(" * Library for " toupper($2) " module ");[14;2H      print(" * Generated automatically by genlib.c.awk at:" strftime(format));[15;8Hprint(" */");[16;2H      print("");[K[16;153H|[17;8Hprint("#include <lib" tolower($2) ".h>");[18;2H      print(""[C;[K[18;153H|[19;5H}[20;2H[K[20;153H|[20;97H[K[20;153H|[21;5Hif ($1 == "$") {[22;2H      print ""[K[22;153H|[23;153H[24;2H      if ($3 == "RW") { loops = 2; set = 1; }[25;8Hif ($3 == "RO") { loops = 1; set = 0; }[K[25;153H|[26;8Hif ($3 == "WO") { loops = 1; set = 1; }[27;2H[K[27;153H|[27;97H[K[27;153H|[28;8Hfor (i=0; i<loops; i++) {[K[28;153H|[29;11Hif (set == 1) {[30;2H            printf("%s","int SET_");    [31;10H } else {[32;10H    printf("%s","int GET_");[33;10H }[K[33;153H|[34;11Hif ($7 == "1") {[35;2H            if ($5 == "8") print [C2 "(void *handle, long *" tolower($2) ") {"[36;10H    if ($5 == "4") print $2 "(void *handle, int *" tolower($2) ") {"[37;10H    if [C$5 == "2") print $2 "(void *handle, short *" tolower($2) ") {"[38;14Hif ($5 == "1") print $2 "(void *handle, char *" tolower($2) ") {"[39;2H[K[39;153H|[39;14Hprint("   int offset = 0;");[40;10H } else {[K[40;153H|[41;10H    if [C$5 == "8") print $2 "(void *handle, int offset, long *" tolower($2) ") {"[42;10H    if [C$5 == "4") print $2 "(void *handle, int offset, int *" tolower($2) ") {"[43;10H    if [C$5 == "2") print $2 "(void *handle, int offset, short *" tolower($2) ") {"[44;10H    if [C$5 == "1") print $2 "(void *handle, int offset, char *" tolower($2) ") {"
|[45;11H}
|[46;11Hprint("   struct vmeio_riob_s riob;");
|[47;11Hprint("   riob.winum[C[C=" $6 ";");
|[48;11Hprint("   riob.offset =" $4 " + offset;");
|[49;11Hprint("   riob.bsize[C[C=" $5 ";");
|[50;11Hprint("   riob.buffer =" tolower($2) ";");
|[51;11Hprint("   return DMA(handle,&riob," set ");");
|[52;11Hprint("}");
|[53;11Hprint("");
|[54;11Hset = 0;
|[55;8H}
|   }
|}
|[58;153H
|END {[61;1H[K[61;96H[K[62;51Hgenmake.awk[2;2H[62;42H2[3;2H[62;42H3[4;2H[62;42H4[5;2H[62;42H5[6;2H[62;42H6[7;2H[62;42H7[8;2H[62;42H8[9;2H[62;44H2[9;3H[62;44H3[9;4H[62;44H4[9;5H[62;44H5[9;6H[62;44H6[9;7H[62;44H7[9;8H[62;44H8[9;9H[62;44H9[9;10H[62;44H10[9;11H[62;45H1[9;12H[62;45H2[9;13H[62;45H3[9;14H[62;45H4[9;15H[62;45H5[9;16HLNAME") {[62;45H6[9;17HINAME") {[62;45H7[9;18HBNAME") {[62;45H8[9;19H[62;42H9[C[C [10;2H[10;59r[59;1H
[1;62r[10;2H[59;1H|   print ""[59;153H|[61;1H[K[61;96H[K[10;2H[10;59r[59;1H
[1;62r[10;2H[59;1H|}[59;153H|[61;1H[K[61;96H[K[10;2H[10;59r[59;1H
