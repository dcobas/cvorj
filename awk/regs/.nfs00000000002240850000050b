$(LIBNAME).$(CPU).o: $(SRCS)

$(LIBNAME).h: $(REGFILE) genlib.h.awk
        cat $(REGFILE) | ./genlib.h.awk > $(LIBNAME).h

$(LIBNAME).c: $(REGFILE) genlib.c.awk
        cat $(REGFILE) | ./genlib.c.awk > $(LIBNAME).c

$(LIBNAME).$(CPU).a: $(LIBNAME).$(CPU).o
        -$(RM) $@
        $(AR) $(ARFLAGS) $@ $^
        $(RANLIB) $@

$(LIBNAME).$(CPU).so: $(LIBNAME).$(CPU).o
        -$(RM) $@
        $(CC) $(CFLAGS) -o $@ -shared $^

clean:
        rm -f $(LIBNAME).$(CPU).o
        rm -f $(LIBNAME).$(CPU).a
        rm -f $(LIBNAME).$(CPU).so
        rm -f $(LIBNAME).h
        rm -f $(LIBNAME).c

